{
  "name": "Android Dev Studio",
  "short_name": "DevStudio",
  "description": "Complete OS-like development environment with Flutter/Dart support",
  "version": "3.0.0",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0f172a",
  "theme_color": "#0ea5e9",
  "orientation": "any",
  "categories": ["development", "productivity", "utilities", "education"],
  "lang": "en",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512", 
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "1280x720",
      "type": "image/svg+xml",
      "form_factor": "wide",
      "label": "Desktop Environment"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "640x1136",
      "type": "image/svg+xml", 
      "form_factor": "narrow",
      "label": "Mobile Interface"
    }
  ],
  "shortcuts": [
    {
      "name": "Flutter IDE",
      "short_name": "Flutter",
      "description": "Open Flutter development environment",
      "url": "/?app=flutter-ide",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Terminal",
      "short_name": "Terminal", 
      "description": "Open Ubuntu-like terminal",
      "url": "/?app=terminal",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "APK Builder",
      "short_name": "APK Build",
      "description": "Build Android APK files",
      "url": "/?app=apk-builder", 
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Package Manager", 
      "short_name": "Packages",
      "description": "Install development packages",
      "url": "/?app=package-manager",
      "icons": [
        {
          "src": "data:image/svg+xml;base64,",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.termux",
      "id": "com.termux"
    }
  ],
  "prefer_related_applications": false,
  "display_override": ["window-controls-overlay", "standalone"],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/dart": [".dart"],
        "text/yaml": [".yaml", ".yml"],
        "application/json": [".json"],
        "text/javascript": [".js"],
        "text/typescript": [".ts"]
      }
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": [".dart", ".js", ".ts", ".json", ".yaml", ".yml"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "flutter",
      "url": "/?flutter-url=%s"
    }
  ]
}